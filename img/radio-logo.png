	/* calendar ajax */

	$(document).on("click", "#nextbutton", function(){

		  var nxtmonth =  $(this).attr("rel");

		  var thisyear =  $(this).attr("lang");

		  $("#calendar_container").html("<div style='width:280px;height:255px;'>Loading...</div>");



 				jQuery.ajax({

					url: "https://nightingale.edu.np/wp-content/themes/nightingale/calendar.php",

					type: 'POST',

					data: {

						nextmonth:nxtmonth,

						ntyear:thisyear

					},

					dataType: 'html',

					success: function(response) {

					$("#calendar_container").html(response);

				}

			});

	 });

	 $(document).on("click", "#prevbutton", function(){

		  var prevmonth =  $(this).attr("rel");

		   var thisyear =  $(this).attr("lang");

		  // $("#event_display").hide();

		   $("#calendar_container").html("<div style='width:280px;height:255px;'>Loading...</div>");

		   jQuery.ajax({

					url: "https://nightingale.edu.np/wp-content/themes/nightingale/calendar.php",

					type: 'POST',

					data: {

						action:'calendar',

						prevmonth:prevmonth,

						pvyear:thisyear},

					dataType: 'html',

					success: function(response) {

					$("#calendar_container").html(response);

				}

		  });

	 });

